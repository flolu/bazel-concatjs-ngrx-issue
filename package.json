{
  "dependencies": {
    "@angular/animations": "^11.2.4",
    "@angular/common": "^11.2.4",
    "@angular/core": "^11.2.4",
    "@angular/forms": "^11.2.4",
    "@angular/platform-browser": "^11.2.4",
    "@angular/platform-browser-dynamic": "^11.2.4",
    "@angular/router": "^11.2.4",
    "@ngrx/store": "9.1.2",
    "core-js": "2.6.9",
    "rxjs": "^6.6.3",
    "systemjs": "6.1.2",
    "tslib": "^2.0.1",
    "uuid": "^8.3.2",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular/compiler": "^11.2.4",
    "@angular/compiler-cli": "^11.2.4",
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@bazel/bazelisk": "^1.6.1",
    "@bazel/concatjs": "^3.2.0",
    "@bazel/ibazel": "^0.13.2",
    "@bazel/jasmine": "^3.2.1",
    "@bazel/protractor": "^3.0.0",
    "@bazel/rollup": "^3.2.0",
    "@bazel/terser": "^3.2.0",
    "@bazel/typescript": "^3.2.0",
    "@ngrx/store-devtools": "9.1.2",
    "@types/jasmine": "^3.6.2",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "history-server": "^1.3.1",
    "html-insert-assets": "^0.14.1",
    "jasmine": "^3.6.3",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-requirejs": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.8",
    "patch-package": "^6.2.2",
    "prettier": "^2.1.2",
    "protractor": "^7.0.0",
    "requirejs": "^2.3.6",
    "rollup": "^2.28.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "terser": "^5.3.4",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "start": "ibazel run //services/client:dev_server",
    "prod": "bazelisk run //services/client:app_server",
    "upgrade-ngrx": "yarn add @ngrx/store -D @ngrx/store-devtools",
    "postinstall": "patch-package && ngcc"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "arrowParens": "avoid",
    "semi": false
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
